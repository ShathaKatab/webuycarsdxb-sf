{
  "name": "majidmvulle/webuycars",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-4": { "": "src/" },
    "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
  },
  "require": {
    "php": ">=5.6.22",
    "doctrine/dbal": "<2.5",
    "doctrine/doctrine-bundle": "~1.4",
    "doctrine/doctrine-migrations-bundle": "^1.0",
    "doctrine/migrations": "^1.0",
    "doctrine/mongodb-odm": "^1.1",
    "doctrine/mongodb-odm-bundle": "^3.1",
    "doctrine/orm": "~2.2,>=2.2.3,<2.5",
    "friendsofsymfony/user-bundle": "~2.0@dev",
    "gedmo/doctrine-extensions": "~2.4@stable",
    "guzzlehttp/guzzle": "^6.0",
    "incenteev/composer-parameter-handler": "~2.0",
    "jms/di-extra-bundle": "^1.7",
    "jms/i18n-routing-bundle": "~1.1@stable",
    "jms/job-queue-bundle": "~1.1@stable",
    "jms/serializer-bundle": "^1.1",
    "sensio/distribution-bundle": "~4.0",
    "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
    "sonata-project/admin-bundle": "^3.1",
    "sonata-project/doctrine-extensions": "^1.0",
    "sonata-project/doctrine-orm-admin-bundle": "^3.1",
    "swiftmailer/swiftmailer": "^5.4",
    "symfony/assetic-bundle": "^2.8",
    "symfony/monolog-bundle": "~2.4",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/symfony": "2.8.*"
  },
  "require-dev": {
    "sensio/generator-bundle": "~3.0",
    "symfony/phpunit-bridge": "~2.7"
  },
  "scripts": {
    "symfony-scripts": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "config": {
    "bin-dir": "bin",
    "platform": {
      "php": "5.6.22"
    },
    "sort-packages": true
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": {
      "dev-master": "2.8-dev"
    }
  },
  "repositories": [
    {
      "url": "https://github.com/jordisala1991/SonataUserBundle",
      "type": "git"
    }
  ]
}
