        <div class="container-fluid">
            <div id="footer-menu-container" class="row">
                <div id="footer-menu" class="col-md-6">
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2">
                            <ul class="list-unstyled">
                                {% for key, value in site_menu %}
                                    <li><a href="/{{ attribute(site_menu, key).slug }}">{{ attribute(site_menu, key).title }}</a></li>
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                </div>
                <div id="footer-contact" class="col-md-6 col-xs-12 text-center">
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2 col-xs-12">
                            <div class="thumbnail">
                                <img class="img-responsive" onerror="this.src='{{ asset('images/webuycarsdxb-logo.png') }}'" src="{{ asset('images/webuycarsdxb-logo.svg') }}" alt="{{ site_title }}">

                                <div class="caption">
                                    <h2 class="text-left">Headquarters</h2>

                                    <p class="text-left">
                                        {{ company_name }}<br>
                                        {{ company_office_number }},&nbsp;{{ company_office_building }},&nbsp;{{ company_office_street }}<br>
                                        PO Box: {{ company_po_box }} Dubai<br>
                                        Tel: {{ company_office_phone }}
                                    </p>

                                    {% if getBranches()|length %}
                                        <h2 class="text-left">Inspection Branch</h2>
                                        {% for branch in getBranches() %}
                                            <p class="text-left">
                                                {{ branch.name }}<br>
                                                {{ branch.address|nl2br }}<br>
                                                Tel: {{ branch.phoneNumber }}
                                            </p>
                                        {% endfor %}
                                    {% endif %}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="footer-strip" class="container-fluid bg-grey">
            <div class="row">
                <div class="col-md-6 col-md-offset-0 col-xs-10 col-xs-offset-2">
                    <ul class="list-inline list-unstyled">
                        {% if site_facebook is defined and site_facebook is not null %}
                            <li><a target="_blank" href="{{ site_facebook }}"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                        {% endif %}
                        {% if site_twitter is defined and site_twitter is not null %}
                            <li><a target="_blank" href="{{ site_twitter }}"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                        {% endif %}
                        {% if site_instagram is defined and site_instagram is not null %}
                            <li><a target="_blank" href="{{ site_instagram }}"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                        {% endif %}
                        {% if site_linkedin is defined and site_linkedin is not null %}
                            <li><a target="_blank" href="{{ site_linkedin }}"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                        {% endif %}

                    </ul>
                </div>
                <div class="col-md-6 col-xs-12">
                    <h4 class="text-right"><i class="fa fa-copyright" aria-hidden="true"></i> {{ "now"|date("Y") }} {{ company_name }}</h4>
                </div>
            </div>
        </div>
    </footer>
{% endblock %}
{% block javascripts %}
    {% javascripts filter='?jsqueeze' output='js/app-v1.js'
        'assets/vendor/@bower_components/ngmap/build/scripts/ng-map.js'

        'assets/js/angular/valuation/controllers.controller.js'

    %}
        <script src="{{ asset_url }}"></script>
        <script>
            var theToggle = document.getElementById('toggle');


            // hasClass
            function hasClass(elem, className) {
                return new RegExp(' ' + className + ' ').test(' ' + elem.className + ' ');
            }
            // addClass
            function addClass(elem, className) {
                if (!hasClass(elem, className)) {
                    elem.className += ' ' + className;
                }
            }
            // removeClass
            function removeClass(elem, className) {
                var newClass = ' ' + elem.className.replace( /[\t\r\n]/g, ' ') + ' ';
                if (hasClass(elem, className)) {
                    while (newClass.indexOf(' ' + className + ' ') >= 0 ) {
                        newClass = newClass.replace(' ' + className + ' ', ' ');
                    }
                    elem.className = newClass.replace(/^\s+|\s+$/g, '');
                }
            }
            // toggleClass
            function toggleClass(elem, className) {
                var newClass = ' ' + elem.className.replace( /[\t\r\n]/g, " " ) + ' ';
                if (hasClass(elem, className)) {
                    while (newClass.indexOf(" " + className + " ") >= 0 ) {
                        newClass = newClass.replace( " " + className + " " , " " );
                    }
                    elem.className = newClass.replace(/^\s+|\s+$/g, '');
                } else {
                    elem.className += ' ' + className;
                }
            }

            theToggle.onclick = function() {
                toggleClass(this, 'on');
                return false;
            }
        </script>

    {% endjavascripts %}
{% endblock %}
</body>


