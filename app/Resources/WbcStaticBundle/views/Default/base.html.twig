<body>
{% include 'WbcStaticBundle:partials:google-tag-manager-body.html.twig' %}
{% block navigation %}
    <nav class="navbar navbar-default">
        <div class="container">
            <div class="row">
                {#Desktop Menu#}
                <div class="col-md-12 hidden-xs hidden-sm">
                    <div class="col-md-3 col-md-offset-0 col-xs-6 col-xs-offset-3">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="{{ path('wbc_static_default_index') }}">
                                <img class="img-responsive" onerror="this.src='{{ asset('images/webuycarsdxb-logo.png') }}'" alt="{{ site_title }}" src="{{ asset('images/webuycarsdxb-logo.svg') }}">
                            </a>
                        </div>
                    </div>
                    <div class="col-md-1 hidden-md hidden-lg">
                        <button type="button" class="navbar-toggle" data-toggle="collapse"
                                data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <div class="navbar-collapse collapse col-md-12">
                            <ul class="nav navbar-nav list-unstyled">
                                {% for key, value in site_menu %}
                                    <li>
                                        <a href="/{{ attribute(site_menu, key).slug }}">{{ attribute(site_menu, key).title }}</a>
                                    </li>
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                    <div id="navbar-steps-container" class="col-md-5 col-xs-12">
                        <div class="h2">We buy any car in 30 min!</div>
                    </div>
                    <div id="navbar-phone-container" class="col-md-3 col-md-pull-0 col-xs-pull-9 text-right">
                        {% if craue_setting('phoneNumber1') is not null and craue_setting('phoneNumber1') != '' %}
                            <a href="tel: {{ craue_setting('phoneNumber1') }}">
                                <i class="glyphicon glyphicon-earphone" aria-hidden="true"></i>
                                &nbsp;<span>{{ craue_setting('phoneNumber1') }}</span>
                            </a>
                        {% endif %}
                        {% if craue_setting('phoneNumber2') is not null and craue_setting('phoneNumber2') != '' %}
                            <br/><a href="tel: {{ craue_setting('phoneNumber2') }}">
                            <span>{{ craue_setting('phoneNumber2') }}</span>
                        </a>
                        {% endif %}
                    </div>
                    <div class="col-md-1 hidden-xs hidden-sm">
                        <a href="#menu" id="toggle"><span></span></a>
                        <div id="menu">
                            <ul >
                                {% for key, value in site_menu %}
                                    <li>
                                        <a href="/{{ attribute(site_menu, key).slug }}">{{ attribute(site_menu, key).title }}</a>
                                    </li>
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                </div>

                {#Mobile Menu#}
                <div class="col-md-12 hidden-md hidden-lg">
                    <div class="row">
                        <div class="col-xs-8 col-xs-offset-2 col-sm-6 col-sm-offset-3">
                            <div class="navbar-header">
                                <a class="navbar-brand mobile" href="{{ path('wbc_static_default_index') }}">
                                    <img class="img-responsive" alt="{{ site_title }}"
                                         onerror="this.src='{{ asset('images/webuycarsdxb-logo.png') }}'"
                                         src="{{ asset('images/webuycarsdxb-logo.svg') }}">
                                </a>
                            </div>
                        </div>
                    </div>

                    <div id="second-row-container" class="row">
                        <div id="navbar-phone-container" class="col-xs-9 text-left">
                            {% if craue_setting('phoneNumber1') is not null and craue_setting('phoneNumber1') != '' %}
                                <a href="tel: {{ craue_setting('phoneNumber1') }}">
                                    <i class="glyphicon glyphicon-earphone" aria-hidden="true"></i>
                                    &nbsp;<span>{{ craue_setting('phoneNumber1') }}</span>
                                </a>
                            {% endif %}
                            {% if craue_setting('phoneNumber2') is not null and craue_setting('phoneNumber2') != '' %}
