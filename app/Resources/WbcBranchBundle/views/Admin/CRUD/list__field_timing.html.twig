{% extends '@WbcBranch/Admin/base_list_field.html.twig' %}

{% block field %}
    {% if object.bookedAt is not null and object.bookedAt != false %}
        {% if (object.status == 'new' or object.status == 'callback')
        and object.bookedAt|date('m/d/Y') == "now"|date('m/d/Y')
         %}
            <img style="width: 10px; height: 10px; vertical-align: middle;" src="{{ asset('images/roundgreen-blinker.gif') }}">
        {% endif %}
        {{ object.bookedAtTiming }}
    {% endif %}
{% endblock %}
